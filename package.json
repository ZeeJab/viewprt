{
  "name": "viewprt",
  "version": "1.1.0",
  "description": "tiny, high performance viewport position & intersection observer",
  "author": "Garth Poitras <garth22@gmail.com>",
  "license": "MIT",
  "main": "dist/viewprt.js",
  "module": "dist/viewprt.es.js",
  "jsnext:main": "dist/viewprt.es.js",
  "files": [
    "dist/viewprt.js",
    "dist/viewprt.es.js"
  ],
  "keywords": [
    "viewport",
    "observer",
    "scroll",
    "in-view",
    "viewability",
    "lazy-load",
    "infinite-scroll"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gdub22/viewprt.git"
  },
  "scripts": {
    "dev": "rollup --config --watch",
    "build": "rollup --config",
    "build:test": "rollup --config rollup.config.test.js",
    "lint": "eslint src test --fix",
    "test:js": "yarn run build:test && mocha --require test/helpers/env.js",
    "test": "yarn lint && yarn test:js",
    "prepublish": "yarn build",
    "clean": "rm -rf dist",
    "demo": "open demos/index.html"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.13.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "rewire": "^2.5.2",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-watch": "^3.2.2"
  },
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "browser": true,
      "mocha": true
    }
  }
}
